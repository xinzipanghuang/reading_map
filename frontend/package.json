{
  "name": "knowledge-dag-builder",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "cross-env ELECTRON_DEV=true electron electron-main.cjs",
    "electron:build": "npm run build && electron-builder"
  },
  "dependencies": {
    "vue": "^3.3.4",
    "axios": "^1.6.2",
    "vis-network": "^9.1.9",
    "element-plus": "^2.4.4",
    "@element-plus/icons-vue": "^2.3.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.0",
    "vite": "^5.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "electron": "^31.0.0",
    "electron-builder": "^24.13.3",
    "cross-env": "^7.0.3"
  },
  "main": "electron-main.cjs",
  "build": {
    "appId": "com.readmap.knowledge",
    "productName": "ReadMap",
    "main": "electron-main.cjs",
    "directories": {
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "electron-main.cjs",
      "../backend/**/*"
    ],
    "win": {
      "target": "nsis"
    }
  }
